{
  "name": "vue-connector",
  "version": "0.0.8",
  "description": "ðŸª¡ Connect anything to your vue components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nooooooom/vue-connect-anything.git"
  },
  "bugs": {
    "url": "https://github.com/nooooooom/vue-connect-anything/issues"
  },
  "keywords": [
    "vue",
    "hoc",
    "connect"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "nooooooom <https://github.com/nooooooom>",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prepublish": "vitest run && pnpm build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "dependencies": {
    "vue-forward-ref": "^0.0.7"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "jsdom": "^21.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.28.3",
    "vue": "^3.2.47",
    "vue2": "npm:vue@^2.7.0"
  },
  "peerDependencies": {
    "typescript": ">=4.4.4",
    "vue": "^2.7.0 || ^3.2.0"
  }
}
